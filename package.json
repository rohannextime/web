{
  "name": "rohan",
  "version": "0.0.0-development",
  "description": "Apresentando Rohan, o seu novo companheiro nas estradas. Inspirado na vasta e mágica terra de Rohan do universo de Tolkien, este aplicativo foi projetado para ser o guardião e amigo fiel de todos os motoristas. Assim como os cavaleiros de Rohan defendem sua terra e seu povo, nosso aplicativo está aqui para proteger, guiar e servir todos os que se aventuram nas estradas",
  "private": true,
  "keywords": [],
  "author": "Pedro Duarte - NeXTIME",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "vercel-build": "pnpm install && pnpm build",
    "build": "turbo run build --parallel",
    "clean:all": "turbo run clean && pnpm store prune && rm -rf .next .turbo node_modules tsconfig.tsbuildinfo pnpm-lock.yaml",
    "coverage": "turbo run coverage",
    "e2e": "turbo run e2e",
    "test": "turbo run test",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --parallel",
    "audit": "pnpm audit",
    "outdated": "pnpm outdated",
    "release": "semantic-release",
    "semantic-release:ui": "semantic-release --config semantic-release-ui.config.js",
    "release:ui": "turbo run build --filter=ui && changeset publish",
    "reset:all": "pnpm clean:all && pnpm i && turbo run build --parallel",
    "start": "turbo run start",
    "typecheck": "turbo run typecheck --parallel",
    "update": "pnpm update --latest",
    "dunharrow:build": "turbo run build --filter dunharrow",
    "dunharrow:dev": "turbo run dev --filter dunharrow",
    "edoras:build": "turbo run build --filter edoras",
    "edoras:dev": "turbo run dev --filter edoras",
    "edoras:e2e": "turbo run e2e --filter edoras",
    "edoras:e2e:ui": "turbo run e2e:ui --filter edoras",
    "edoras:e2e:chromium": "turbo run e2e:chromium --filter edoras",
    "edoras:e2e:debug": "turbo run e2e:debug --filter edoras",
    "edoras:e2e:codegen": "turbo run e2e:codegen --filter edoras",
    "edoras:e2e:report": "turbo run e2e:report --filter edoras",
    "westfold:build": "turbo run build --filter westfold",
    "westfold:dev": "turbo run dev --filter westfold",
    "westfold:e2e": "turbo run e2e --filter westfold",
    "westfold:e2e:ui": "turbo run e2e:ui --filter westfold",
    "westfold:e2e:chromium": "turbo run e2e:chromium --filter westfold",
    "westfold:e2e:debug": "turbo run e2e:debug --filter westfold",
    "westfold:e2e:codegen": "turbo run e2e:codegen --filter westfold",
    "westfold:e2e:report": "turbo run e2e:report --filter westfold",
    "ui:build": "turbo run build --filter ui",
    "ui:dev": "turbo run dev --filter ui",
    "precommit": "turbo lint typecheck audit",
    "commit": "cz",
    "prepare": "husky install",
    "postinstall": "husky install",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@next/font": "^14.1.4",
    "@nextime-ui/icons": "^2.6.2",
    "@nextime-ui/react": "^2.6.2",
    "@nextime-ui/theme-tools": "^2.6.2",
    "@nextime-ui/tokens": "^2.6.2",
    "@t3-oss/env-nextjs": "^0.9.2",
    "lucide-react": "^0.367.0",
    "next": "^14.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@nextime-ui/eslint-config": "^2.6.2",
    "@nextime-ui/ts-config": "^2.6.2",
    "@playwright/test": "^1.43.0",
    "@rohan/ui": "workspace:^",
    "@storybook/addon-essentials": "^8.0.6",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/blocks": "^8.0.6",
    "@storybook/manager-api": "^8.0.6",
    "@storybook/react": "^8.0.6",
    "@storybook/react-vite": "^8.0.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.0.6",
    "@swc/core": "^1.4.13",
    "@turbo/gen": "^1.13.2",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "@types/prop-types": "^15.7.12",
    "@types/react": "18.2.75",
    "@types/react-dom": "18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.13.2",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "semantic-release": "^23.0.8",
    "semantic-release-monorepo": "^8.0.2",
    "storybook": "^8.0.6",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "turbo": "^1.13.2",
    "typescript": "5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rohannextime/web.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
